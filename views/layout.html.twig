<!DOCTYPE html>
<html>
<head>
    <title>Auto-Solid {{ title }}</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=no">
    <meta name="apple-mobile-web-app-capable" content="yes">

    <link rel="icon" type="image/png" href="{{ url('start') }}public/img/logo-color.png">
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,300i,400,400i,700,700i&amp;amp;subset=latin-ext">
    <link rel="stylesheet" type="text/css" href="{{ url('start') }}public/fonts/fontawesome/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="{{ url('start') }}public/css/bundle.css">

    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <![endif]-->
</head>

<body>

    <div class="page-wrap">

    {% include('header.html.twig') %}

    {% include(currentPage ~ '.html.twig') %}

    {% include('footer.html.twig') %}

    </div>

    <script type="text/javascript" src="{{ url('start') }}public/js/bundle.js"></script>

    {% if scrollTo is defined and scrollTo is not empty %}
        <script>
            $(document).ready(function(){
                WebUI.scrollTo('{{ scrollTo }}');
            });
        </script>
    {% endif %}
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
        ga('create', 'UA-6352174-3', 'auto');
        ga('send', 'pageview');
    </script>
</body>
</html>